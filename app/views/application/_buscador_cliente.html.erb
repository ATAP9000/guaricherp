<div class="modal fade" id="buscador_cliente" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
	<h5 class="modal-title" id="modal-items-title">Buscador de clientes</h5>
	<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body-items modal-body" id="modal-body-items">
	<div class="form-group">
	  <label for="buscador">Ingresa el Nombre o Numero de Cedula/RIF</label>
	  <input type="text" class="form-control" data-venta = "<%= @venta.id %>" id="buscador_clientes" placeholder="Nombre cliente...">
	</div>
	<br>
	<table class="table table-dark" id="tabla_buscador_clientes" >
	  <thead>
	    <tr>
	      <th>Cedula/RIF</th>
	      <th>Nombre</th>
	      <th>Accion</th>
	    </tr>
	  </thead>
	  <tbody>

	    
	  </tbody>
	</table>
      </div>
      <div class="modal-footer" id="modal-footer-items">
	<button type="button" class="btn btn-dark" data-bs-dismiss="modal">Cerrar</button>
      </div>
    </div>
  </div>
</div>
