<div class="modal fade" id="buscador_proveedor" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modal-items-title">Buscador de proveedores</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body-items modal-body" id="modal-body-items">
        <div class="form-group">
	  <label for="buscador">Ingresa el nombre del proveedor</label>
	  <input type="text" class="form-control" data-compra = "<%= @compra.id %>" id="buscador_proveedores" placeholder="Nombre proveedor...">
	</div>
	<table class="table table-dark" id="tabla_buscador_proveedores" >
	  <thead>
	    <tr>
	      <th>Numero Rif</th>
	      <th>Nombre</th>
	      <th></th>
	    </tr>
	  </thead>
	  <tbody>


	  </tbody>
	</table>
      </div>
      <div class="modal-footer" id="modal-footer-items">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
      </div>
    </div>
  </div>
</div>
