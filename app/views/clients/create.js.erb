$('#modalClientes').modal('toggle');
$('#tablaClientes').append(
    '<tr id="cliente_<%= @client.id %>" ><td><%= @client.rif_cliente %></td><td><%= @client.nombre_cliente %></td><td><%= @client.phoneclients.first.try(:numero_cliente) %></td><td><%= link_to "Ver", client_path(@client), html_options = {class: "btn btn-info btn-xs", remote: true} %> <%= link_to 'Editar', edit_client_path(@client), html_options = {class: "btn btn-info btn-xs", remote: true} %> <%= link_to "Borrar", @client, remote: true, method: :delete, data: { confirm: "Borrar cliente: #{@client.nombre_cliente}?"}, class: "btn btn-danger btn-xs" %></td></tr>'
);